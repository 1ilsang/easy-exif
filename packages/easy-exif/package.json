{
  "name": "easy-exif",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "1ilsang <1ilsang.dev@gmail.com>",
  "license": "MIT",
  "description": "Easily extract and edit image EXIF data.",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "lint": "pnpm run '/^lint:.*/'"
  },
  "keywords": [
    "exif",
    "image",
    "metadata"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  },
  "engines": {
    "node": ">=22.18.0"
  }
}
